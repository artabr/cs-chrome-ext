    "node-jq": "^2.3.5",
    "npm-run-all": "^4.1.5",
    "prettier": "2.7.1",
    "rollup": "2.79.1",
    "sass": "1.55.0",
    "ts-jest": "29.0.2",
    "ts-loader": "9.4.1",
    "typescript": "4.8.3",
    "vite": "3.1.3",
    "ws": "8.9.0"
