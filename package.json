{
  "name": "cs-chrome-ext",
  "version": "1.0.1-beta.11",
  "description": "Adds useful enhancements to the Contentstack admin panel UI",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/artabr/cs-chrome-ext.git"
  },
  "private": "true",
  "scripts": {
    "build": "tsc --noEmit && vite build",
    "build:watch": "cross-env __DEV__=true vite build --watch",
