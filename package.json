    "wss": "node utils/reload/initReloadServer.js",
    "dev": "pnpm run build:hmr && (run-p wss build:watch)",
    "test": "jest"
  },
  "type": "module",
  "dependencies": {
    "@extend-chrome/storage": "^1.5.0",
    "react": "18.2.0",
